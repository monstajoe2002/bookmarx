{
  "manifest_version": 3,
  "name": "Bookmarx",
  "description": "A simple open-source bookmark manager for Chrome/Chromium.",
  "version": "0.1.3",
  "action": {
    "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "activeTab"]
}
